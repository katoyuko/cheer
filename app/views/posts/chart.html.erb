<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h2>Chart</h2>
      <p class="small-title-chart">頑張りチャート</p>
      <p class="chart">月別投稿件数</p>
        <%= column_chart @chart_data,
          colors: ["#76d9cd"],
          suffix: "件",
          messages: {empty: "投稿がありません"}
         %>
    </div>
  </div>
</div>
